{
  "version": "1.2.3",
  "name": "@ivuorinen/stylelint-a11y",
  "description": "Plugin for stylelint with a11y rules",
  "main": "dist/index.js",
  "repository": "https://github.com/ivuorinen/stylelint-a11y.git",
  "author": "Stanislav <botev.st@gmail.com>",
  "maintainers": [
    {
      "name": "Ismo Vuorinen",
      "url": "https://github.com/ivuorinen"
    }
  ],
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "a11y"
  ],
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "prettify": "prettier '**/*.js' --write",
    "prepublish": "npm run build",
    "pretest": "npm run lint -- --fix",
    "test": "npx jest",
    "coverage": "npx jest --coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "stylelint": "^8 || ^9 || ^10 || ^11 || ^12 || ^13 || ^14"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.16.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.0",
    "babel-preset-jest": "^27.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "extend": ">=3.0.2",
    "jest": "^27.0.0",
    "jest-cli": "^27.0.0",
    "jest-preset-stylelint": "^5.0.2",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "stylelint": "^14.8.0"
  }
}
